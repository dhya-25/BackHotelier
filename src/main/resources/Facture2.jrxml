	JOIN "RESIDENT" ON 
	 "ARRANGEMENT"."RESIDENT_ID_RESIDENT" = "RESIDENT"."ID_RESIDENT" 
	JOIN "CONSOMATION" ON 
	 "CONSOMATION"."RESIDENTS_ID_RESIDENT" = "RESIDENT"."ID_RESIDENT" 
